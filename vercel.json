{
  "version": 2,
  "builds": [
    {
      "src": "app/pricing/pricing.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/api/pricing", "methods": ["POST"], "dest": "app/pricing/pricing.py" },
    { "src": "/api/pricing-db", "methods": ["GET"], "dest": "app/pricing/pricing.py" }
  ]
}
